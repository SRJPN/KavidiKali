<!DOCTYPE html>
<html>

<head>
    <title>Kavidikali</title>
    <link rel="stylesheet" href="styles/kavidikali.css">
    <link rel="stylesheet" href="styles/gameOver.css">

    <link rel="stylesheet" href="styles/common.css">
    <script type="text/javascript" src="js/jquery-2.1.4.min.js"></script>
    <script type="text/javascript" src="js/kavidikali.js"></script>
    <style>
        .footer {
            position: static;
        }
    </style>
</head>

<body>
<div class="header">
  <% include ./partials/header %>
</div>

<div class="body">

    <div class="board"></div>


    <div class="playInfo">
        <div class="myInfo_head">Players</div>
        <ul class="players">
        </ul>
        <div class="myInfo">
            <div id="myName" class="myInfo_head">Sooraj Parameswaran</div>
            <div class="myInfos">
                <div class="myInfo_head">Enter Board</div>
                <div id="enterBoard">
                </div>
            </div>
            <div class="myInfos">
                <div class="myInfo_head">First Cut</div>
                <div id="firstCut">Pending</div>

            </div>
            <div class="myInfos" style="border:none">
                <div class="myInfo_head">Final Temple</div>
                <div id="reachedDestination">
                </div>
            </div>
        </div>
        <div class="notifications_head"> Notifications</div>
        <div class="notifications">
            <div id="notifications"></div>
        </div>
        <div class="dice_area">
            <div id="dice_values">
                <div id="last_dice_value"></div>
                <div id="diceValues"></div>
            </div>
            <div class="dice">
                <img src="images/dice.png" id="dice">
            </div>
        </div>
    </div>


</div>
<div class="gameOver"></div>

<div class="footer">
    <% include ./partials/footer  %>
</div>
</body>

</html>
